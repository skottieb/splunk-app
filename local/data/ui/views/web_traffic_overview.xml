<form theme="dark">
  <label>Web Traffic Overview</label>
  <fieldset submitButton="false" autoRun="false">
    <input type="time" token="time_selection" searchWhenChanged="true">
      <label>Time Range</label>
      <default>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </default>
    </input>
    <input type="dropdown" token="location_selection" searchWhenChanged="true">
      <label>Location</label>
      <choice value="*">All</choice>
      <fieldForLabel>location</fieldForLabel>
      <fieldForValue>location</fieldForValue>
      <search>
        <query>`z-web`|stats count by location</query>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </search>
      <default>*</default>
    </input>
    <input type="dropdown" token="department_selection" searchWhenChanged="true">
      <label>Department</label>
      <fieldForLabel>department</fieldForLabel>
      <fieldForValue>department</fieldForValue>
      <search>
        <query>`z-web` location = $location_selection$ |stats count by department</query>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </search>
      <choice value="*">All</choice>
      <default>*</default>
    </input>
    <input type="dropdown" token="user_selection" searchWhenChanged="true">
      <label>User</label>
      <choice value="*">All</choice>
      <default>*</default>
      <fieldForLabel>user</fieldForLabel>
      <fieldForValue>user</fieldForValue>
      <search>
        <query>`z-web` location = $location_selection$ department = $department_selection$ | where user != location |stats count by user</query>
      </search>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>LOCATIONS WITH ACTIVITY</title>
      <single>
        <search>
          <query>`z-web` location = $location_selection$ department = $department_selection$ user = $user_selection$ | timechart  dc(location) as unique_locations</query>
          <earliest>$time_selection.earliest$</earliest>
          <latest>$time_selection.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="colorBy">trend</option>
        <option name="colorMode">none</option>
        <option name="drilldown">none</option>
        <option name="rangeColors">["0x53a051","0x53a051"]</option>
        <option name="rangeValues">[0]</option>
        <option name="refresh.display">progressbar</option>
        <option name="trendColorInterpretation">inverse</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="trendInterval">-24h</option>
        <option name="useColors">0</option>
      </single>
    </panel>
    <panel>
      <title>AUTHENTICATED USERS SEEN</title>
      <single>
        <search>
          <query>`z-web` location = $location_selection$ department = $department_selection$ user = $user_selection$ | timechart dc(user) as unique_users</query>
          <earliest>$time_selection.earliest$</earliest>
          <latest>$time_selection.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="colorBy">trend</option>
        <option name="colorMode">none</option>
        <option name="drilldown">none</option>
        <option name="height">113</option>
        <option name="rangeColors">["0x53a051","0x53a051"]</option>
        <option name="rangeValues">[0]</option>
        <option name="refresh.display">progressbar</option>
        <option name="showSparkline">1</option>
        <option name="trendColorInterpretation">inverse</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="trendInterval">-24h</option>
        <option name="useColors">0</option>
      </single>
    </panel>
    <panel>
      <title>TOTAL TRANSACTIONS</title>
      <single>
        <search>
          <query>`z-web` location = $location_selection$ department = $department_selection$ user = $user_selection$ | timechart count as transactions</query>
          <earliest>$time_selection.earliest$</earliest>
          <latest>$time_selection.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="colorBy">trend</option>
        <option name="colorMode">none</option>
        <option name="drilldown">none</option>
        <option name="rangeColors">["0x53a051","0x53a051"]</option>
        <option name="rangeValues">[0]</option>
        <option name="refresh.display">progressbar</option>
        <option name="trendColorInterpretation">inverse</option>
        <option name="trendDisplayMode">percent</option>
        <option name="useColors">0</option>
      </single>
    </panel>
    <panel>
      <title>TOTAL BYTES</title>
      <single>
        <search>
          <query>`z-web` location = $location_selection$ department = $department_selection$ user = $user_selection$ | eval totalsize = ((responsesize + requestsize)/(1000000000)) |  timechart sum(totalsize) as total_bytes</query>
          <earliest>$time_selection.earliest$</earliest>
          <latest>$time_selection.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="colorBy">trend</option>
        <option name="colorMode">none</option>
        <option name="drilldown">none</option>
        <option name="numberPrecision">0.0</option>
        <option name="rangeColors">["0x53a051","0x53a051"]</option>
        <option name="rangeValues">[0]</option>
        <option name="refresh.display">progressbar</option>
        <option name="trendColorInterpretation">inverse</option>
        <option name="trendDisplayMode">percent</option>
        <option name="unit">GB</option>
        <option name="useColors">0</option>
      </single>
    </panel>
  </row>
  <row>
    <panel>
      <html>
       <u1> </u1>
     </html>
    </panel>
  </row>
  <row>
    <panel>
      <title>WEB TRAFFIC OVER TIME</title>
      <input type="dropdown" token="parameter_selection" searchWhenChanged="true">
        <label>Parameter</label>
        <choice value="count">Transactions</choice>
        <choice value="sum(requestsize)">Sent Bytes</choice>
        <choice value="sum(responsesize)">Received Bytes</choice>
        <default>count</default>
      </input>
      <input type="dropdown" token="url_class_selection" searchWhenChanged="true">
        <label>URL Class</label>
        <choice value="*">All</choice>
        <choice value="&quot;Bandwidth Loss&quot;">Bandwidth Loss</choice>
        <choice value="&quot;Business Use&quot;">Business Use</choice>
        <choice value="&quot;General Surfing&quot;">General Surfing</choice>
        <choice value="&quot;Legal Liability&quot;">Legal Liability</choice>
        <choice value="&quot;Productivity Loss&quot;">Productivity Loss</choice>
        <search>
          <query/>
        </search>
        <fieldForLabel>urlclass</fieldForLabel>
        <fieldForValue>urlclass</fieldForValue>
      </input>
      <input type="dropdown" token="urlcategory_selection" searchWhenChanged="true">
        <label>URL Category</label>
        <search>
          <query>`z-web` location = $location_selection$ department = $department_selection$ urlclass = $url_class_selection$ | where urlclass != "Advanced Security Risk" | stats count by urlcategory</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <fieldForLabel>urlcategory</fieldForLabel>
        <fieldForValue>urlcategory</fieldForValue>
        <choice value="*">All</choice>
        <default>*</default>
        <initialValue>*</initialValue>
      </input>
      <chart>
        <search>
          <query>`z-web` location = $location_selection$ department = $department_selection$ user = $user_selection$ urlclass = $url_class_selection$ urlcategory = $urlcategory_selection$ | timechart $parameter_selection$</query>
          <earliest>$time_selection.earliest$</earliest>
          <latest>$time_selection.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">collapsed</option>
        <option name="charting.axisTitleY.visibility">collapsed</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.abbreviation">none</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.abbreviation">auto</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.abbreviation">none</option>
        <option name="charting.axisY2.enabled">0</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.chart">line</option>
        <option name="charting.chart.bubbleMaximumSize">50</option>
        <option name="charting.chart.bubbleMinimumSize">10</option>
        <option name="charting.chart.bubbleSizeBy">area</option>
        <option name="charting.chart.nullValueMode">gaps</option>
